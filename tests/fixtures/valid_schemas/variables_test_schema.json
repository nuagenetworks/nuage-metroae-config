{
  "title": "Schema validator for Nuage Metro Levistate template Variables Testing",
  "$id": "urn:nuage-metro:levistate:template:variables-testing",
  "required": [
    "name",
    "select_name",
    "number",
    "true_or_false",
    "fruit",
    "string_list",
    "int_list",
    "soda_list"
  ],
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ipv6_address": {
      "type": "string",
      "title": "Ipv6 address"
    },
    "soda_list": {
      "items": {
        "enum": [
          "coke",
          "pepsi",
          "sprite"
        ]
      },
      "type": "array",
      "title": "Soda list"
    },
    "name": {
      "type": "string",
      "title": "Name"
    },
    "string_list": {
      "items": {
        "type": "string"
      },
      "type": "array",
      "title": "String list"
    },
    "select_name": {
      "type": "string",
      "title": "Select name"
    },
    "int_list": {
      "items": {
        "type": "integer"
      },
      "type": "array",
      "title": "Int list"
    },
    "number": {
      "type": "integer",
      "title": "Number"
    },
    "any_ip_address": {
      "type": "string",
      "title": "Any ip address"
    },
    "ipv4_address": {
      "type": "string",
      "title": "Ipv4 address"
    },
    "true_or_false": {
      "type": "boolean",
      "title": "True or false"
    },
    "fruit": {
      "enum": [
        "apple",
        "orange",
        "banana"
      ],
      "title": "Fruit"
    }
  }
}
